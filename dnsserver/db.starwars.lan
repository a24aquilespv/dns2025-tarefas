;
; Zona directa para starwars.lan
;
$TTL    604800
@    IN    SOA    darthvader.starwars.lan. admin.starwars.lan. (
                  17        ; Serial
             604800        ; Refresh
              86400        ; Retry
            2419200        ; Expire
             604800 )    ; Negative Cache TTL
;
@    IN    NS    darthvader.starwars.lan.
@    IN    NS    darthsidious.starwars.lan.

starwars.lan.    IN  A 192.168.20.10 
darthvader  IN  A   172.24.0.100
darthsidious    IN  A   172.24.0.101
skywalker   IN  A   192.168.20.101
skywalker   IN  A   192.168.20.111
luke    IN  A   192.168.20.22
yoda    IN  A   192.168.20.24
yoda    IN  A   192.168.20.25
c3p0    IN  A   192.168.20.26
chewbacca IN A  192.168.20.28
palpatine   IN  CNAME   darthsidious.starwars.lan.
@   IN  MX  10  c3p0.starwars.lan.
lenda   IN TXT "Hola dns secundario" 
test	IN	TXT	"Forzar transferencia de zona: dig axfr <zona> @ip_dns_autoritativo"

; Subdominio virtual antigarepublica
$ORIGIN antigarepublica.starwars.lan.

xendor  IN  A 192.168.20.222
ardenlyn  IN  A 192.168.20.223

; Delegar subdominio imperio a leia
$ORIGIN imperio.starwars.lan.

@ IN  NS  leia
leia IN  A 172.24.0.110
